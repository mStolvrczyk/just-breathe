{
  "name": "air-quality-check",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "run-p build:**",
    "build:app": "vue-cli-service build",
    "build:lambda": "netlify-lambda build server/lambda",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "client": "vue-cli-service serve",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "heroku-postbuild": "npm install && npm run build",
    "server": "nodemon server/index.js",
    "start": "node server",
    "unit": "jest"
  },
  "dependencies": {
    "@mdi/font": "^3.9.97",
    "animated-number-vue": "^1.0.0",
    "apexcharts": "^3.17.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "celebrate": "^10.0.1",
    "chart.js": "^2.8.0",
    "concurrently": "^5.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.6.5 ",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "leaflet": "^1.5.1",
    "leaflet-geometryutil": "^0.9.1",
    "leaflet-geosearch": "^2.7.0",
    "leaflet-knn": "^0.1.0",
    "material-icons": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.26.1",
    "register-service-worker": "^1.7.1 ",
    "vue": "^2.6.10",
    "vue-apexcharts": "^1.5.2",
    "vue-chartjs": "^3.4.2",
    "vue-pull-refresh": "^0.2.7",
    "vue-router": "^3.1.2",
    "vue-svg-gauge": "^1.2.1",
    "vue2-leaflet": "^2.2.1",
    "vue2-leaflet-geosearch": "^1.0.6",
    "vue2-leaflet-locatecontrol": "^1.0.1",
    "vuetify": "^2.1.5",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/register": " ^7.9.0 ",
    "@vue/cli-plugin-babel": " ^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": " ^4.3.1 ",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "http-proxy-middleware": "^1.0.3",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.1",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.2",
    "serverless-http": "^2.3.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.42.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "template-curly-spacing": "off",
      "indent": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
